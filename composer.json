{
	"name": "yurii-github/my-library",
	"type": "project",
	"description": "My book library",
	"license": "GPL-3.0-only",
	"minimum-stability" : "stable",
	"require": {
		"php": "~8.3",
		"ext-fileinfo": "*",
		"ext-gd": "*",
		"ext-intl": "*",
		"ext-json": "*",
		"ext-mbstring": "*",
		"ext-pdo": "*",
		"doctrine/dbal": "^3.0",
		"guzzlehttp/psr7": "^2.7",
		"http-interop/http-factory-guzzle": "^1.0",
		"illuminate/database": "^8.0",
		"illuminate/events": "^8.0",
		"illuminate/validation": "^8.0",
		"monolog/monolog": "^2.2",
		"ramsey/uuid": "^4.1",
		"slim/slim": "^4.6",
		"twig/twig": "^3.1"
	},
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"require-dev": {
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "^11.4"
	},
	"config": {
		"sort-packages": true
	}
}
