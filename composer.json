{
	"name": "yurii-github/my-library",
	"type": "project",
	"description": "My book library",
	"license": "GPL-3.0-only",
	"minimum-stability" : "stable",
	"version": "2.0.0",
	"require": {
		"php": ">=7.4",
		"ext-json": "*",
		"ext-pdo": "*",
		"ext-gd": "*",
		"ext-mbstring": "*",
		"guzzlehttp/psr7": "*",
		"http-interop/http-factory-guzzle": "*",
		"twig/twig": "^3.1",
		"illuminate/database": "^8.0",
		"symfony/dotenv": "^5.1",
		"symfony/translation": "^5.1",
		"slim/slim": "^4.6",
		"ext-fileinfo": "*",
		"illuminate/validation": "^8.0",
      	"ext-intl": "*",
		"illuminate/events": "^8.0",
		"ramsey/uuid": "^4.1",
		"monolog/monolog": "^2.2"
    },
	"autoload": {
		"psr-4": {
			"App\\": "src/"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Tests\\": "tests/"
		}
	},
	"require-dev": {
		"mikey179/vfsstream": "^1.6",
		"phpunit/phpunit": "~9.0"
	},
	"config": {
		"sort-packages": true
	}
}
